<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.j2003.mapper.SysUserMapper">

    <select id="findUserByLoginName" parameterType="string" resultType="com.qf.j2003.po.SysUser">
        select * from tb_sys_user where loginName =#{username}
    </select>
</mapper>